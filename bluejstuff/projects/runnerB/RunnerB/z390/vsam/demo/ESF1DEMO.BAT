REM BUILD VSAM ESDS CLUSTER VCDT ESF1.390 AND RUN ESF1SP, ESF1SG, COMP1
REM RUN ESF1SP SEQ PUT TO BUILD ESF1.VES
SET QFILE=vsam\demo\ESF1.TF1
SET VFILE=vsam\demo\DEMOCAT.ESF1
ERASE vsam\demo\ESF1.VES
CALL ASMLG vsam\demo\ESF1SP %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESF1SP
REM RUN ESF1SG SEQ GET FROM ESF1.VES TO ESF1.TF2
ERASE vsam\demo\ESF1.TF2
SET VFILE=vsam\demo\DEMOCAT.ESF1
SET QFILE=vsam\demo\ESF1.TF2
CALL ASMLG vsam\demo\ESF1SG %1 %2 %3 %4 %5
IF ERRORLEVEL 1 PAUSE ERROR IN ESF1SG
REM COMPARE INPUT AND OUTPUT QSAM FILES
CALL rt\COMP1 vsam\demo\ESF1.TF1 vsam\demo\ESF1.TF2 vsam\demo\ESF1DEMO.DIF 
IF ERRORLEVEL 1 PAUSE ESF1.TF1 NE ESF1.TF2
